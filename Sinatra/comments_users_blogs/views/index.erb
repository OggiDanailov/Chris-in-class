
<% @blogs.each do |blog| %>

<a href="/blog/<%= blog.id %>"><div><%= blog.title %></div></a>
<div><%= blog.content %></div>

<% end %>


<form action="/profile" method="post">
	<input type="text" name="title" placeholder="title">
	<input type="text" name="content" placeholder="content">
	<input type="submit">
</form>
